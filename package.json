{
  "name": "react",
  "version": "1.0.0",
  "description": "React example starter project",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.js",
  "dependencies": {
    "mxgraph": "4.1.0",
    "mxgraph-js": "1.0.1",
    "react": "16.12.0",
    "react-color": "2.18.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "format": "npx @biomejs/biome format --write ./src",
    "lint": "npx @biomejs/biome lint --apply ./src"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tests/e2e/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@playwright/test": "^1.44.0",
    "@types/node": "^20.12.12",
    "jest": "24.7.1",
    "lefthook": "1.6.11"
  }
}
